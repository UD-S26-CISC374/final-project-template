{
    "name": "final-project",
    "version": "5.0.0",
    "type": "module",
    "description": "CISC374 Project",
    "homepage": "https://github.com/UD-S26-CISC374/final-project-template#readme",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "tsc -b && vite build vite/config.prod.mjs",
        "lint": "eslint ./src",
        "lint:fix": "eslint  --fix ./src",
        "eslint-output": "eslint-output ./src --ext .tsx --ext .ts --max-warnings 0",
        "format": "prettier --config .prettierrc --write src/**/*.{ts,tsx}",
        "preview": "vite preview",
        "test": "jest --watch --config=jest.config.js",
        "test:cov": "jest --coverage --config=jest.config.js"
    },
    "keywords": [
        "Phaser",
        "Phaser 3",
        "Phaser3",
        "html5 game",
        "TypeScript",
        "webpack",
        "starter"
    ],
    "author": {
        "name": "Austin Cory Bart",
        "url": "https://github.com/acbart"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/UD-S26-CISC374/final-project-template.git"
    },
    "license": "MIT",
    "dependencies": {
        "phaser": "^3.90.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "terser": "^5.28.1",
        "@eslint/js": "^9.36.0",
        "@swc/core": "^1.13.5",
        "@swc/jest": "^0.2.39",
        "@testing-library/cypress": "^8.0.7",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^24.6.0",
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.4",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-output": "^4.0.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.22",
        "globals": "^16.4.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-transform-stub": "^2.0.0",
        "prettier": "^3.6.2",
        "prettier-eslint": "^16.4.2",
        "sass": "^1.60.0",
        "stylelint": "^14.16.1",
        "stylelint-config-rational-order": "^0.0.4",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-order": "^5.0.0",
        "ts-node": "^10.9.2",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.7"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!src/main.tsx"
        ]
    }
}
